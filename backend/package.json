{
    "name": "quo-integrations-frigg-backend",
    "prettier": "@friggframework/prettier-config",
    "private": true,
    "scripts": {
        "test": "jest --group=-interactive --group=-live-api",
        "test:full": "jest",
        "test:process": "jest --testPathPattern process-tests process.test",
        "test:unit": "jest --config jest.unit.config.js",
        "test:auth": "jest --testPathPattern=test/integrations/AsanaIntegration.auth.test.js",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --group=-interactive --group=-live-api --ci",
        "debug": "jest --testTimeout 0",
        "ci": "npm install",
        "lint": "eslint .",
        "lint:fix": "prettier --write --loglevel error . && eslint . --fix",
        "lint:cloudformation": "sls package --stage dev && cfn-lint .serverless/cloudformation-template-*.json",
        "lint:markdown": "markdownlint --fix .",
        "format": "prettier --write --loglevel error .",
        "backend-start": "sls offline --stage dev --config serverless.js",
        "backend-start:staging": "sls offline --stage staging --config serverless.js",
        "docker:start": "docker compose up -d",
        "docker:stop": "docker compose down",
        "knip": "knip",
        "frigg:start": "frigg start",
        "frigg:deploy": "AWS_PROFILE=lefthook-nagaris frigg deploy",
        "frigg:db:setup": "frigg db:setup"
    },
    "dependencies": {
        "@friggframework/core": "2.0.0--canary.454.4c72357.0",
        "@friggframework/api-module-frigg-scale-test": "0.2.0-next.1",
        "axios": "^1.6.0",
        "node-cache": "^5.1.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@friggframework/devtools": "2.0.0--canary.454.4c72357.0",
        "@friggframework/eslint-config": "2.0.0--canary.454.4c72357.0",
        "@friggframework/prettier-config": "2.0.0--canary.454.4c72357.0",
        "@friggframework/serverless-plugin": "2.0.0--canary.454.4c72357.0",
        "@friggframework/test": "2.0.0--canary.454.4c72357.0",
        "@types/node": "^22.1.0",
        "chai": "^4.5.0",
        "chai-http": "^4.4.0",
        "dotenv": "^16.4.5",
        "jest": "^29.7.0",
        "knip": "^5.27.1",
        "markdownlint-cli": "^0.41.0",
        "nock": "^13.5.4",
        "prettier": "3.5.3",
        "serverless-dotenv-plugin": "^6.0.0",
        "serverless-jetpack": "0.11.2",
        "serverless-kms-grants": "^1.1.0",
        "serverless-offline": "^13.8.0",
        "serverless-offline-sqs": "^8.0.0",
        "typescript": "^5.5.4",
        "webpack": "^5.93.0"
    }
}
