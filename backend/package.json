{
    "name": "quo-integrations-frigg-backend",
    "prettier": "@friggframework/prettier-config",
    "private": true,
    "scripts": {
        "test": "jest --group=-interactive --group=-live-api",
        "test:full": "jest",
        "test:process": "jest --testPathPattern process-tests process.test",
        "test:unit": "jest --config jest.unit.config.js",
        "test:auth": "jest --testPathPattern=test/integrations/AsanaIntegration.auth.test.js",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --group=-interactive --group=-live-api --ci",
        "debug": "jest --testTimeout 0",
        "ci": "npm install",
        "lint": "eslint .",
        "lint:fix": "prettier --write --loglevel error . && eslint . --fix",
        "lint:cloudformation": "sls package --stage dev && cfn-lint .serverless/cloudformation-template-*.json",
        "lint:markdown": "markdownlint --fix .",
        "format": "prettier --write --loglevel error .",
        "backend-start": "sls offline --stage dev --config serverless.js",
        "backend-start:staging": "sls offline --stage staging --config serverless.js",
        "docker:start": "docker compose up -d",
        "docker:stop": "docker compose down",
        "knip": "knip",
        "frigg:start": "frigg start",
        "frigg:deploy": "frigg deploy",
        "frigg:db:setup": "frigg db:setup"
    },
    "dependencies": {
        "@friggframework/api-module-attio": "^2.0.0-canary.25.13cc249.0",
        "@friggframework/api-module-frigg-scale-test": "^0.2.0-next.2",
        "@friggframework/core": "2.0.0--canary.461.0b53aff.0",
        "axios": "^1.6.0",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "express-async-handler": "^1.2.0",
        "node-cache": "^5.1.2",
        "serverless-http": "^2.7.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@friggframework/devtools": "^2.0.0--canary.461.0b53aff.0",
        "@friggframework/eslint-config": "v2.0.0-next.44",
        "@friggframework/prettier-config": "v2.0.0-next.44",
        "@friggframework/serverless-plugin": "^2.0.0--canary.461.0b53aff.0",
        "@friggframework/test": "v2.0.0-next.44",
        "@prisma/client": "^6.17.0",
        "@types/node": "^22.1.0",
        "chai": "^4.5.0",
        "chai-http": "^4.4.0",
        "dotenv": "^16.4.5",
        "esbuild": "^0.25.11",
        "jest": "^29.7.0",
        "knip": "^5.27.1",
        "markdownlint-cli": "^0.41.0",
        "nock": "^13.5.4",
        "osls": "^3.40.1",
        "prettier": "3.5.3",
        "prisma": "^6.17.0",
        "serverless-dotenv-plugin": "^6.0.0",
        "serverless-esbuild": "^1.55.1",
        "serverless-jetpack": "0.11.2",
        "serverless-kms-grants": "^1.1.0",
        "serverless-offline": "^13.8.0",
        "serverless-offline-sqs": "^8.0.0",
        "typescript": "^5.5.4",
        "webpack": "^5.93.0"
    }
}
